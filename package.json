{
  "name": "matches-selector-ng",
  "version": "1.0.1",
  "description": "Check if a DOM element matches a given selector, with decent browser support and unit tests.",
  "main": "js/index.js",
  "scripts": {
    "prepare": "rimraf js/ && babel -s inline -d js/ src/ --ignore '*.test.*' && flow-copy-source -v src js --ignore '*.test.*'",
    "test": "yarn run lint && yarn run flow_check && jest",
    "flow_check": "flow check",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "keywords": [
    "browserify",
    "CSS",
    "selector"
  ],
  "bugs": {
    "url": "https://github.com/AgentME/matches-selector-ng/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgentME/matches-selector-ng.git"
  },
  "homepage": "https://github.com/AgentME/matches-selector-ng#readme",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "eslint": "^5.0.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "flow-bin": "^0.80.0",
    "flow-copy-source": "^2.0.0",
    "jest": "^23.1.0",
    "rimraf": "^2.6.2"
  }
}
